@{
    ViewBag.Title = "Contacto";
}

<div class="titulo">
    <h1>
        <i>@ViewBag.Title</i>
    </h1>
</div>
<h3>@ViewBag.Message</h3>

<address>
    One Microsoft Way<br />
    Redmond, WA 98052-6399<br />
    <abbr title="Phone">P:</abbr>
    425.555.0100
</address>

<address>
    <strong>Support:</strong>   <a href="mailto:Support@example.com">Support@example.com</a><br />
    <strong>Marketing:</strong> <a href="mailto:Marketing@example.com">Marketing@example.com</a>
</address>

<hr />
<h2>¡Mandanos un mensaje!</h2>
<hr />

<div class="mapa">
    <div class="forma">
        <form action="ejemplo.php" method="get">
            <p>Email: <input type="text" name="email" size="40"></p>
            <p>Nombre: <input type="text" name="nombre" size="40"></p>
            <p>Mensaje: <textarea name="texto" rows="12" cols="80"></textarea></p>
            <div class="rb">
                <h4>Hombre</h4>
                <input type="radio" name="hm" value="h">
                <h4>Mujer</h4>
                <input type="radio" name="hm" value="h">
            </div>
            <div class="botones">
                <input class="boton" type="submit" value="Enviar">
                <input class="boton" type="reset" value="Borrar">
            </div>
        </form>
    </div>
    <hr />
    <div>
        <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d54217.30304783445!2d-116.62131199999997!3d31.863603199999996!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1ses!2smx!4v1606511683916!5m2!1ses!2smx" width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
    </div>
</div>